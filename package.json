{
  "name": "z3-solver-jsrp",
  "version": "4.15.4",
  "description": "z3-solver fork used in js-randomness-predictor",
  "browser": "dist/build/browser.js",
  "main": "dist/build/node.js",
  "types": "dist/build/node.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE.txt"
  ],
  "directories": {
    "doc": "doc",
    "example": "examples"
  },
  "scripts": {
    "prepublishOnly": "npm run build:server && npm run build:browser",
    "install:js": "cd src/api/js && npm install",
    "build:server": "rm -rf dist && npm run server:prebuild && npm --prefix ./src/api/js run build:server:src:api:js && npm run server:postbuild",
    "build:browser": "npm run browser:prebuild && npm --prefix ./src/api/js run build:browser:src:api:js && npm run browser:postbuild",
    "browser:prebuild": "node jsrp/prebuild.browser.js",
    "browser:postbuild": "node jsrp/postbuild.browser.js",
    "server:prebuild": "node jsrp/prebuild.server.js",
    "server:postbuild": "node jsrp/postbuild.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthewoestreich/z3.git"
  },
  "author": "Microsoft and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthewoestreich/z3/issues"
  },
  "homepage": "https://github.com/matthewoestreich/z3#readme",
  "dependencies": {
    "async-mutex": "^0.5.0"
  }
}
